  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg": "^2.5.0",
    "egg-bin": "^4.5.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^4.19.1",
    "eslint-config-egg": "^7.0.0",
